<?xml version="1.0" encoding="utf-8"?>
<extensions>
    <extension class="com.generalbytes.batm.server.extensions.extra.decent.AbstractDecentExtension">
        <wallet prefix="dctd" name="DCT Wallet Adapter">
            <param name="protocol" />
            <param name="user" />
            <param name="password" />
            <param name="host" />
            <param name="port" />
            <cryptocurrency>DCT</cryptocurrency>
        </wallet>
        <exchange prefix="dct_bittrex" name="Bittrex.com Exchange (ext)">
            <param name="apikey" />
            <param name="apisecret" />
            <param name="replacements" />
            <param name="intermediates" />
            <cryptocurrency>DCT</cryptocurrency>
            <cryptocurrency>BTC</cryptocurrency>
        </exchange>
        <ratesource prefix="dct_bittrex" name ="Bittrex.com Rate Source (ext)" >
            <param name="intermediates" />
            <cryptocurrency>DCT</cryptocurrency>
        </ratesource>
        <cryptologo cryptocurrency="DCT" file="dct.png"/>
    </extension>
</extensions>
