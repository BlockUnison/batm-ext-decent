<?xml version="1.0" encoding="utf-8"?>
<extensions>
    <extension class="com.generalbytes.batm.server.extensions.extra.decent.AdapterDecentExtension">
        <wallet prefix="dctd" name="DCT Wallet Adapter">
            <param name="protocol" />
            <param name="user" />
            <param name="password" />
            <param name="host" />
            <param name="port" />
            <cryptocurrency>DCT</cryptocurrency>
        </wallet>
        <!--<exchange prefix="dummy" name="Dummy Exchange">-->
            <!--<cryptocurrency>DCT</cryptocurrency>-->
        <!--</exchange>-->
        <exchange prefix="btrx" name="Bittrex.com Exchange">
            <param name="apikey" />
            <param name="apisecret" />
            <cryptocurrency>DCT</cryptocurrency>
            <cryptocurrency>BTC</cryptocurrency>
        </exchange>
        <ratesource prefix="fixed" name ="Fixed Price" >
            <param name="rate" />
            <cryptocurrency>DCT</cryptocurrency>
        </ratesource>
        <ratesource prefix="btrx" name ="Bittrex Rate Source" >
            <cryptocurrency>DCT</cryptocurrency>
        </ratesource>
        <cryptologo cryptocurrency="DCT" file="dct.png"/>
    </extension>
</extensions>
